{
    "name": "Getting Started Sutil Example",
    "description": "Build an Extension with Sutil!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/logo-small.png"
        }
    },
    "icons": {
        "128": "/images/logo-small.png"
    }
}
